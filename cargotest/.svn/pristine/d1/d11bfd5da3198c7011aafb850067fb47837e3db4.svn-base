*{
    list-style-type:none;
}
 
a:link{text-decoration:none}        /*超链接状态*/
a:visited{text-decoration:none}    /*超链接被点击过的状态*/
a:hover{text-decoration:none}     /*鼠标移至超链接上方的状态*/
a:active{text-decoration:none}     /*鼠标点击超链接时的状态*/

#pageone .page{
     width:260px;
     margin:0 auto;
     margin-top:20px;
}
#pageone .page .input-row{
    padding:5px;
}

#pageone .page .input-row .input-content{
    display:inline-block;
    width:150px;
}

#pageone .page .input-row label{
    display:inline-block;

}

#pageone .page .input-row .select-left{
    display:inline-block;
    float:left;
    width:50px;
    
}
#pageone .page .input-row .select-left select{
    width:60px;
}
#pageone .page .input-row .select-right select{
    width:60px;
}

#pageone .page .input-row .input-text{
    display:inline-block;
    padding:5px;
    font-size:18px;
    text-shadow: 0 -1px 1px #FFFFFF;
}

input[readonly]{
  background-color:#FFFFFF;   
}

#pageone .page .input-row .label-name,.label-phone,.label-carNum{
    width:150px;
    font-weight: 900;
    font-style: italic;
}

.input-row .mybtn{
    display:inline-block;
    margin-right:30px; 
    float:right;
}

.ui-btn-icon-left{
    display:inline-block;
    width:80px;   
    height:40px;
    
}